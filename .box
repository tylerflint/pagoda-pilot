writable_directories: [ test2 
  what]

some_bad_node:
  - bad indent 1
   - bad indent 5

version: '5.2.17'

extensions: [eaccelerator, mysql, mysqli] 

document_root: '/'

index_list: [index.html, index.php]

far_future_expires: false

error_pages:
    "404": "http://pilot.pagodabox.com/info.php$uri"
   
url_rewrites:
   	###   ^index\.php$ - [L]
    - "^index.php$ last"

	### uploaded files
	### RewriteRule ^([_0-9a-zA-Z-]+/)?files/(.+) wp-includes/ms-files.php?file=$2 [L]
    - "^/([_0-9a-zA-Z-]+/)?files/(.+) /wp-includes/ms-files.php?file=$2 last"
    
        rule: "^/ last"
        conditions:
            - "!-f $request_filename"
            - "!-d $request_filename"       
    
   	###   ^[_0-9a-zA-Z-]+/(wp-(content|admin|includes).*) $1 [L]
    - "^/[_0-9a-zA-Z-]+/(wp-(content|admin|includes).*) /$1 last"

   	###   ^[_0-9a-zA-Z-]+/(.*\.php)$ $1 [L]
    - "^/[_0-9a-zA-Z-]+/(.*\.php)$ /$1 last"

   	###   . index.php [L]
    - ". index.php last"

